{
  "kind": "collectionType",
  "collectionName": "quotations",
  "info": {
    "singularName": "quotation",
    "pluralName": "quotations",
    "displayName": "quotation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 3
    },
    "phone": {
      "type": "string",
      "required": true,
      "maxLength": 14,
      "minLength": 9,
      "regex": "[0-9]+"
    },
    "message": {
      "type": "text",
      "maxLength": 250
    },
    "email": {
      "type": "email",
      "required": true,
      "maxLength": 254
    },
    "pdfVoucher": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "files"
      ]
    },
    "products": {
      "type": "json"
    },
    "status_quotation": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::status-quotation.status-quotation",
      "inversedBy": "quotations"
    },
    "code_quotation": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
